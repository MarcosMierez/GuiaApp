

<div class="center-align">
    <h2 class="headers">
        Acesso de Palestrante
    </h2>
</div><form action="" method="post">

    <div class="row">
        <div class="input-field col s12">
            <input id="emailPalestrante" name="emailPalestrante" class="validate" type="text">
            <label class="active" for="emailPalestrante">Email</label>
        </div>
    </div>
    <div class="row">
        <div class="input-field col s12">
            <input id="senhaPalestrante" name="senhaPalestrante" class="validate" type="password">
            <label class="active" for="senhaPalestrante">Senha</label>
        </div>
    </div>
    <div class="row">
        <div class="input-field col s6">
            <button type="submit" class="waves-effect waves-light btn ">Logar</button>
        </div>
        <div class="right-align">
            <div class="input-field col s6">
                <a href="@Url.Action("Cadastrar","Palestrante",new{Area="Cadastro"})" class="waves-effect waves-light btn orange ">Registrar</a>
            </div>
        </div>
    </div>

</form>
